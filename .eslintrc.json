{
	"extends": ["airbnb", "prettier"],
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"plugins": ["react", "prettier"],
	"rules": {
		"prettier/prettier": "warn",
		"quotes": [2, "single"],
		"import/no-extraneous-dependencies": "off"
	}
}
